[.Reservations | .[].Instances | select(.[].State.Name != "terminated") | {
    name: (.[].Tags? + []) | from_entries | .Name?,
    instance: .[].InstanceId,
    state: .[].State.Name,
    type: .[].InstanceType,
    public_address: .[].PublicIpAddress,
    private_address: .[].NetworkInterfaces | .[].PrivateIpAddress
}]
